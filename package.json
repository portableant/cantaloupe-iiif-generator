{
  "name": "cantaloupe-iiif-generator",
  "version": "1.0.0",
  "description": "A Node.js tool to generate IIIF Presentation 3.0 manifests from Directus collections for use with Cantaloupe IIIF server",
  "main": "generator.js",
  "scripts": {
    "generate": "node generator.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "generate:iiif": "node src/generator.js",
    "generate:iiif-main": "node src/generator-main.js",
    "start": "npm run generate:iiif",
    "annotations": "npm run generate:iiif-main",
    "create:index": "node src/create-index.js"
  },
  "keywords": [
    "iiif",
    "cantaloupe",
    "directus",
    "manifest",
    "digital-humanities",
    "image-server",
    "cultural-heritage"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.12.0"
  },
  "devDependencies": {},
  "engines": {
    "node": ">=14.0.0"
  }
}
